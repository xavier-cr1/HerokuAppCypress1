{
  "name": "herokuappcypress1",
  "version": "1.0.0",
  "description": "cypress e2e test solution for herokuexpress server",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "cy:open:docker": "cypress open --config-file cypress/config/appSettingsDocker.json",
    "cy:open:local": "cypress open --config-file cypress/config/appSettingsLocal.json",
    "cy:open:heroku": "cypress open --config-file cypress/config/appSettingsHeroku.json",
    "cy:run:docker": "cypress run --config-file cypress/config/appSettingsDocker.json",
    "cy:run:local": "cypress run --config-file cypress/config/appSettingsLocal.json",
    "cy:run:heroku": "cypress run --config-file cypress/config/appSettingsHeroku.json"
  },
